@{
}
@model List<Room>;

<div class="container">
    <h2>History</h2>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th class="my-1" scope="col">RoomName</th>
                <th class="my-1" scope="col">Capacity</th>
                <th class="my-1" scope="col">Type</th>
                <th class="my-1" scope="col">Fair</th>
                <th class="my-1" scope="col">isBooked</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                @foreach (Room room in Model)
                {
                    <td>@room.RoomName</td>
                    <td>@room.Capacity</td>
                    <td>@room.Type</td>
                    <td>@room.Fair</td>
                    <td>@room.IsBooked</td>
                }
            </tr>
        </tbody>
    </table>
</div>